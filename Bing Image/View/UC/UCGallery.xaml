<UserControl x:Class="Bing_Image.View.UC.UCGallery"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars" 
             xmlns:properties="clr-namespace:Bing_Image.Properties"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Background="AliceBlue">
        <WebBrowser Name="gallry" Margin="0,27,0,0"  />
        <dxb:MainMenuControl VerticalAlignment="Top" Name="MMcontrol">
             <dxb:BarSubItem x:Name="btnOpenFolder" CategoryName="BuiltInMenus" Content="{x:Static properties:Resources.BtnOpenFolder}" ItemClick="btnOpenFolder_Click" />
             <dxb:BarSubItem x:Name="btnClose" CategoryName="BuiltInMenus" Content="{x:Static properties:Resources.BtnClose}" ItemClick="btnClose_Click"/>

        </dxb:MainMenuControl>
          
    </Grid>
</UserControl>
